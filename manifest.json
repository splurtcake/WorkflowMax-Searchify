{
  "manifest_version": 2,
  "name": "WorkflowMax Searchify",
  "version": "1.0",
  "content_scripts": [{
    "matches": [
      "*://*.workflowmax.com/my/timesheet.aspx*",
      "http://localhost/sandbox/workflowmax-bookmarklet/"
    ],
    "js": ["js/vendor/jquery.min.js", "js/vendor/select2.min.js", "js/module.js"],
    "css": ["css/select2.css"],
    "run_at": "document_end"
  }],
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  }
}
